<h1><%=t :attachments %></h1>
<div class="attachments">
  <table class="attachments">
    <% for attachment in @page.attachments.find_without_types( "video", "image" ) %>
      <%= render attachment %>
    <% end %>
  </table>

  <span class="add_attachment">
    <%= render partial: 'add_attachment' %>
  </span>

</div>
